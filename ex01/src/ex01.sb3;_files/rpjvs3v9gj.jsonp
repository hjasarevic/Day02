window['wistiajsonp-/embed/medias/rpjvs3v9gj.jsonp'] = {"media":{"assets":[{"type":"original","slug":"original","display_name":"Original file","details":{},"width":1920,"height":1080,"size":10769238,"bitrate":1606,"public":true,"status":2,"progress":1.0,"url":"https://embed-ssl.wistia.com/deliveries/b5e92f8e9042bc5a57eaa171f69c5745.bin","created_at":1538684780},{"type":"iphone_video","slug":"mp4_h264_329k","display_name":"360p","details":{},"container":"mp4","codec":"h264","width":640,"height":360,"ext":"mp4","size":2211107,"bitrate":329,"public":true,"status":2,"progress":1.0,"metadata":{"max_bitrate":64296,"average_bitrate":47505,"early_max_bitrate":91493},"url":"https://embed-ssl.wistia.com/deliveries/7f8901b0b81a432ed5dfa8e5267ba37f0c905309.bin","created_at":1538684780,"segment_duration":3,"opt_vbitrate":1200},{"type":"mp4_video","slug":"mp4_h264_207k","display_name":"224p","details":{},"container":"mp4","codec":"h264","width":400,"height":224,"ext":"mp4","size":1389836,"bitrate":207,"public":true,"status":2,"progress":1.0,"metadata":{"max_bitrate":11969,"average_bitrate":11348,"early_max_bitrate":13348},"url":"https://embed-ssl.wistia.com/deliveries/980518b7532a0e6c564fdda26ba737d65c00692f.bin","created_at":1538684780,"segment_duration":3,"opt_vbitrate":300},{"type":"md_mp4_video","slug":"mp4_h264_369k","display_name":"540p","details":{},"container":"mp4","codec":"h264","width":960,"height":540,"ext":"mp4","size":2476110,"bitrate":369,"public":true,"status":2,"progress":1.0,"metadata":{"max_bitrate":83723,"average_bitrate":52196,"early_max_bitrate":96131},"url":"https://embed-ssl.wistia.com/deliveries/8fa5ea87d584325fe5352aa7902945e7daef5c1b.bin","created_at":1538684780,"segment_duration":3,"opt_vbitrate":1800},{"type":"hd_mp4_video","slug":"mp4_h264_480k","display_name":"720p","details":{},"container":"mp4","codec":"h264","width":1280,"height":720,"ext":"mp4","size":3221559,"bitrate":480,"public":true,"status":2,"progress":1.0,"metadata":{"max_bitrate":114555,"average_bitrate":66534,"early_max_bitrate":130033},"url":"https://embed-ssl.wistia.com/deliveries/ce36233d3fda5f1f0f5cf1c9a8c978f92699a6a5.bin","created_at":1538684780,"segment_duration":3,"opt_vbitrate":3750},{"type":"hd_mp4_video","slug":"mp4_h264_746k","display_name":"1080p","details":{},"container":"mp4","codec":"h264","width":1920,"height":1080,"ext":"mp4","size":5006129,"bitrate":746,"public":true,"status":2,"progress":1.0,"metadata":{"max_bitrate":189755,"average_bitrate":101168,"early_max_bitrate":218143},"url":"https://embed-ssl.wistia.com/deliveries/bc25ae90b8bdfbeee9c3611bbff415f5e1402d20.bin","created_at":1538684780,"segment_duration":3,"opt_vbitrate":5625},{"type":"storyboard","slug":"storyboard_2000x1243","display_name":"Storyboard","details":{},"width":2000,"height":1243,"ext":"jpg","size":358766,"bitrate":0,"public":true,"status":2,"progress":1.0,"metadata":{"frame_width":200,"frame_height":113,"frame_count":105,"aspect_ratio":1.7777777777777777},"url":"https://embed-ssl.wistia.com/deliveries/9739cf8e19a9785b75761463b2dd6a558fd77d70.bin","created_at":1538684780},{"type":"still_image","slug":"still_image_1280x720","display_name":"Image","details":{},"width":1280,"height":720,"ext":"jpg","size":44515,"bitrate":0,"public":true,"status":2,"progress":1.0,"url":"https://embed-ssl.wistia.com/deliveries/9e886487951fdf158dc0823801eb0aa7b90a8c57.bin","created_at":1538687324}],"distilleryUrl":"https://distillery.wistia.com/x","accountKey":"wistia-production_750265","mediaKey":"wistia-production_37109718","type":"Video","mediaType":"Video","progress":1.0,"status":2,"name":"Getting Started [English]","duration":52.367,"hashedId":"rpjvs3v9gj","branding":false,"enableCustomerLogo":true,"seoDescription":"a Tutorials video","preloadPreference":"","flashPlayerUrl":"https://embed-ssl.wistia.com/flash/embed_player_v2.0.swf?2017-04-20","showAbout":true,"createdAt":1538684780,"firstEmbedForAccount":false,"firstShareForAccount":false,"useMediaDataHostLogic":true,"projectId":3848112,"privacyMode":true,"stats":{"loadCount":146057702,"playCount":18673039,"uniqueLoadCount":52886584,"uniquePlayCount":13360752,"averageEngagement":0.542088},"trackingTransmitInterval":20,"integrations":{},"captions":[{"language":"eng","text":"With Scratch, you can make your own stories,\n\ngames,\n\nand animations.\n\nLet's start by grabbing a \"move\" block.\n\nWe can click the block to run it.\n\nThere are many other blocks to choose from.\nCheck out the different categories.\n\nLet's go to the sound category,\nand grab a \"Play sound until done\" block.\n\nI'll snap this on to our move block\n\nWhen do we want these code blocks to run?\n\nAdd an event block on top to start the code.\n\nI'll add the \"When green flag clicked\" block.\n\nYou can easily reorder\nor delete these blocks at any time.\n\nAdd as many characters as you want\nto your project, and code each one.\n\nCheck out the Tutorials page for ideas and inspiration!"},{"language":"chi","text":"用Scratch你可以创作自己的故事\n\n游戏\n\n和动画。\n\n先试试这个「移动」积木。\n\n点击积木可以运行它。\n\n还有很多别的积木，\n分属不同的类别。\n\n看看声音类别，\n拿起一个「播放声音等待播完」积木。\n\n我把它插在「移动」积木下面\n\n这些积木代码应该何时开始运行呢？\n\n需要在上面放一个事件积木来启动代码。\n\n添加一块「当绿旗被点击」积木。\n\n你随时可以轻易地调整积木的顺序，\n或者删除一个积木。\n\n你可以根据需要添加若干角色，\n编写他们各自的代码。\n\n看看教程中的例子，寻找创意和灵感！"},{"language":"spa","text":"En Scratch puedes crear tus propias historias,\n\njuegos,\n\ny animaciones.\n\nVamos a empezar con un bloque \"mover\".\n\nPuedes hacer clic en el bloque para ejecutarlo.\n\nHay muchos otros bloques para escoger.\nÉchale un vistazo a las diferentes categorías.\n\nVamos a la categoría sonidos,\ny arrastramos un bloque \"tocar sonido hasta que termine\".\n\nVoy a juntarlo después de nuestro bloque mover\n\n¿Cuándo queremos que estos bloques se ejecuten?\n\nAñade un bloque de eventos al principio para iniciar el código.\n\nYo añadiré bloque \"Al hacer clic en bandera verde\".\n\nPuedes reorganizar o eliminar\nestos bloques en cualquier momento.\n\nAñade tantos objetos como quieras\na tu proyecto, y programa cada uno de ellos.\n\n¡Échale un vistazo a la sección de Tutoriales para buscar ideas e inspiración!"},{"language":"amh","text":"ከScratch፣ የራስዎን ታሪክዎች መገንባት ይችላሉ፣\n\nጨዋታዎች፣\n\nእና ዕነማዎች።\n\nለመጀመር፣ አንድ ይእንቅስቃሴ ብሎኬታ እንይዝ።\n\nብሎኬታውን ጠቅ ማድረግ እንችላለን እንድናሰራው።\n\nበርካታ ብሎኬታዎች አሉ።\nየተለያዩ ፈርጆችን ይመለክቱ።\n\nአሁን ወደ የድምጽ ፈርጅ እንሂድ፣\nእና \"ድምጽ አጫውት እስከሚጨረስ\" ብሎኬታውን እንይዝ።\n\nይህ ይኛ እንቅስቃሴ ብሎኬታ ላይ እገጥማለሁ\n\nመቼ ነው እነዚህን እንቅስቃሴ ብሎኬታዎች እንዲሰሩ የምንፈልገው?\n\nየድርጊት ብሎኬታ ይጨምሩ ከላይ ኮዱን ለማስጀመር።\n\nእኔ \"አረንጓዴ ባንዲራ ጠቅ ሲደረግ\" ብሎኬታውን እጨምራለሁ።\n\nበቀላሉ እነዚህን ብሎኬታዎች ማዘጋጀት ዌይስ\nመሰረዝ ይችላሉ።\n\nየሚፈልጉትን ያህል ግለታሪዎች\nወደ ፕሮጄክትዎን ማስገባት ይችላሉ፣ እና ያንዳዱን ኮድ ማድረግ ይችላሉ።\n\nየአግዥ ስልጠናዎች ገጽ ይጎብኙ ለሀሳቦችና ለመኩሪያ!"},{"language":"cat","text":"Amb Scratch, pots crear les teves pròpies històries,\n\njocs,\n\ni animacions.\n\nComenço seleccionant un bloc de \"moviment\".\n\nFaig clic en el bloc per a que funcioni.\n\nHi ha molts més blocs que pots triar.\nRevisa les diferents categories.\n\nVaig a la categoria de So,\ni trio el bloc \"Toca el so fins que acabi\".\n\nEl connectaré amb el bloc de moviment\n\nQuan volem que s'executin aquests blocs de codi?\n\nUn bloc d'esdeveniment al principi per començar el codi.\n\nAfegiré el bloc \"Quan la bandera verda es cliqui\".\n\nPots canviar l'ordre fàcilment\no esborrar els blocs quan vulguis.\n\nAfegeix tants personatges com vulguis\nal projecte, i programa cada un d'ells.\n\nMira la pàgina de Tutorials per a idees i inspiració!"},{"language":"cze","text":"Se Scratch můžeš vytvářet své vlastní příběhy,\n\nhry\n\na animace\n\nZačněme uchopením bloku \"dopředu\".\n\nMůžeme na blok kliknout, aby se spustil.\n\nMáme mnoho dalších bloků pro výběr,\nvyzkoušej různé kategorie.\n\nPojďme na kategorii zvuk\na uchop blok \"Přehraj zvuk až do konce\".\n\nPřipojím jej k našemu bloku dopředu\n\nKdy budeme chtít tyto bloky spustit?\n\nPřidej blok událostí nahoru, pro startování programu.\n\nPřidám blok \"po kliknutí na zelený praporek\".\n\nMůžeš snadno měnit pořadí\nnebo tyto bloky kdykoliv zrušit.\n\nPřidej k projektu postav, kolik chceš\na každou naprogramuj.\n\nVyzkoušej stránku Návody, pro nápady a inspiraci!"},{"language":"fin","text":"Scratchia käyttäen voit luoda omia tarinoita,\n\npelejä,\n\nja animaatioita.\n\nAloitetaan tarttumalla \"liiku\"-lohkoon.\n\nVoimme napsauttaa lohkoa suorittaaksemme sen.\n\nOn myös monia muitakin lohkoja, joista valita.\nTutustu eri kategorioihin.\n\nMennään ääni-kategoriaan,\nja napataan \"soita ääni loppuun\" -lohko.\n\nNappaan tämän seuraavaan liikelohkoomme.\n\nMilloin haluamme, että nämä lohkot suoritetaan?\n\nLisää tapahtumalohko ylimmäksi suorittaaksesi koodin.\n\nLisään \"kun klikataan vihreää lippua\" -lohkon.\n\nVoit helposti milloin vain\njärjestää uudelleen tai poistaa nämä lohkot.\n\nLisää niin monta hahmoa kuin haluat\nprojektiisi ja koodaa jokaista.\n\nTutustu oppitunnit-sivuun saadaksesi ideoita ja inspiraatiota!"},{"language":"fre","text":"Avec Scratch, tu peux créer tes propres histoires,\n\njeux\n\net annimations.\n\nCommençons par prendre un bloc \"avancer\".\n\nOn peut cliquer sur le bloc pour le faire aller.\n\nIl y a plein de blocs possibles.\nRegarde les différentes catégories.\n\nAllons dans la catégorie des sons,\net prenons un bloc \"jouer le son jusqu'au bout\".\n\nJe vais le clipser à notre bloc de mouvement.\n\nQuand veut-on que ces blocs agissent?\n\nAjoute un bloc d'événement au début pour commencer le code.\n\nJe vais ajouter le bloc \"Quand le drapeau vert est cliqué\".\n\nTu peux facilement changer l'ordre\nou supprimer ces blocs à n'importe quel moment.\n\nAjouter autant de personnages que tu veux\nà ton projet, et programme-les chacun.\n\nRegarde la page avec les tutoriels pour plus d'idées et d'inspiration!"},{"language":"gle","text":"Le Scratch, is féidir leat scéalta,\n\ncluichí,\n\nagus beochaintí a chruthú.\n\nAr dtús, faighimis bloc \"bog...\"\n\nCliceáil an bloc chun é a chur i bhfeidhm.\n\nIs féidir leat roghnú ó go leor blocanna eile.\nFéach ar na catagóirí éagsúla.\n\nTéimis go dtí an chatagóir Fuaim,\nagus bloc \"Seinn fuaim go dtí a deireadh\" a roghnú.\n\nCeanglóidh mé an ceann seo leis an mbloc \"bog\" atá ann cheana\n\nCathain a chuirfimid na blocanna seo i bhfeidhm?\n\nCuirimis bloc teagmhais ag an mbarr chun an cód a thosú.\n\nÚsáidfidh mé an bloc \"Nuair a chliceáiltear an bhratach uaine\".\n\nTá sé éasca na blocanna seo\na athordú nó a scriosadh.\n\nCuir an oiread carachtar agus is mian leat leis an tionscadal,\nagus ceangail cód le gach ceann.\n\nGheobhaidh tú tuilleadh smaointe agus spreagadh sna Ranganna Teagaisc!"},{"language":"dut","text":"Met Scratch kun je je eigen verhalen maken,\n\nspelletjes,\n\nen animaties.\n\nLaten we beginnen door een \"neem ... stappen\" blok te slepen.\n\nWe kunnen op het blok klikken om het uit te voeren.\n\nEr zijn veel andere blokken om te kiezen.\nBekijk de verschillende categorieën.\n\nLaten we naar de Geluid categorie gaan\nen een \"start geluid en wacht\" blok slepen.\n\nIk maak het vast aan ons neem ... stappen blok\n\nWanneer willen we deze codeblokken uitvoeren?\n\nVoeg bovenaan een gebeurtenisblok toe om de code te starten.\n\nIk kies het \"wanneer op groene vlag wordt geklikt\" blok\n\nJe kunt deze blokken elk moment\nmakkelijk verwisselen of verwijderen\n\nVoeg zoveel sprites als je wilt\ntoe aan je project en codeer ze\n\nKijk op de Lessen pagina voor ideeën en inspiratie!"},{"language":"slv","text":"S Scratchem lahko ustvariš svoje zgodbe,\n\nigre,\n\nin animacije.\n\nZačnimo tako, da izberemo blok \"pojdi\".\n\nČe kliknemo blok, ga izvedemo.\n\nObstaja mnogo različnih blokov. Oglej si različne kategorije.\n\nNa primer kategorijo Zvok. Tam izberi blok \"Predvajaj zvok do konca\".\n\nIn ga dodaj k bloku pojdi.\n\nKdaj želimo, da se ta dva bloka izvedeta?\n\nNa vrhu dodajmo blok iz kategorije dogodkov.\n\nDodal bom \"ko kliknemo na zeleno zastavo\".\n\nZlahka lahko spremenimo vrstni red\nali zbrišemo te bloke.\n\nV projekt dodaj poljubno število figur\nin za vsako napiši ustrezno kodo.\n\nNa strani z Vodiči poišči ideje in navdih!"},{"language":"swe","text":"Med Scratch, kan du göra dina egna berättelser,\n\nspel,\n\noch animationer.\n\nLåt oss börja med att hämta ett \"gå steg\" - block.\n\nVi kan klicka på blocket för att köra det.\n\nDet finns många andra block att välja mellan.\nKolla in de olika kategorierna.\n\nLåt oss gå till kategorin ljud,\noch hämta ett \"Spela ljud tills färdigt\"-block.\n\nKoppla ihop det med vårt gå-steg-block\n\nNär vill vi att kodblocken ska köras?\n\nLägg till ett händelse-block överst för att starta koden.\n\nJag lägger till \"när gröna flaggan klickas på\" - blocket.\n\nDu kan enkelt ändra ordning\neller ta bort blocken när som helst.\n\nLägg till så många bokstäver som du vill\ntill ditt projekt, och koda var och en.\n\nKolla in sidan för handledningar för fler idéer och inspiration!"},{"language":"zh-Hant","text":"在 Scratch 上，你可以創作屬於自己的故事、\n\n遊戲、\n\n還有動畫。\n\n開始吧，先拖曳出一個「移動」積木。\n\n我們可以在積木上點一下，看看它執行時的樣子。\n\n還有很多其它的積木可以選用。\n逛逛其它不同類別的積木。\n\n我們移到音效類積木，\n拉出「播放音效直到結束」的積木。\n\n我要把它黏合到移動積木。\n\n我們要讓這些指令積木在什麼時候執行？\n\n添加一個事件類積木到程式的最上方。\n\n我要添加「當綠旗被點擊」積木。\n\n你隨時可以調整順序或是刪除這些積木。\n\n要加什麼角色隨你所願\n每個角色上也可以個別撰寫程式。\n\n有空看看我們的教程頁面，\n你會有更多想法還有靈感。"},{"language":"wel","text":"Gyda blociau llithro, gallwch symud\neich cymeriadau o un lle i'r llall.\n\nRhowch eich corlun mewn un man\nac yna tynnu allan floc llithro.\n\nPan fyddwch yn clicio'r bloc bydd y corlun\nyn llithro i'r pwynt yna.\n\nI lithro i ail leoliad,\n\nrhowch y corlun ar bwynt newydd\na llusgo allan floc llithro arall.\n\nI osod pwynt cychwyn,\n\nrhowch y cymeriad lle rydych eisiau cychwyn\na llusgo allan floc mynd iddo a'i roi ar y brig\n\nYn olaf, rwyf am ychwanegu baner werdd\nar y brig i gychwyn y sgript\n\nDewiswch gymeriad a chychwyn llithro!"},{"language":"glg","text":"Cos bloques de esvarar podes mover\na túa personaxe dun sitio a outro.\n\nColoca a túa figura nun sitio\ne arrastra un bloque de esvarar.\n\nAo premer no bloque, a figura\nescorrega até ese sitio.\n\nPara esvarar até outro sitio,\n\ncoloca a figura nun punto novo e\narrastra outro bloque de esvarar.\n\nPara fixar un punto de inicio,\n\ncoloca a personaxe onde queiras comezar,\narrastra un bloque e pono arriba\n\nPor último, engado unha bandeira verde\narriba para iniciar o script\n\nEscolle unha personaxe e comeza a esvarar!"},{"language":"ita","text":"Con i blocchi \"scivola\" puoi far muovere\nun personaggio da un posto all'altro.\n\nPosiziona il tuo sprite nella posizione desiderata\npoi prendi un blocco \"scivola\".\n\nQuando clicchi il blocco, lo sprite\nscivolerà in quel punto.\n\nPer scivolare ad una diversa posizione\n\nposiziona lo sprite nella nuova posizione\ne poi prendi un altro blocco \"scivola\".\n\nPer stabilire un punto di partenza\n\nposiziona il personaggio dove vuoi iniziare\ne poi prendi un blocco \"vai a\" e piazzalo in cima\n\nInfine aggiungiamo un blocco \"bandiera verde\"\nin cima alla sequenza di blocchi per poterla avviare\n\nScegli il personaggio che preferisci e inizia a scivolare!"},{"language":"gre","text":"Με το Scratch, μπορείς να\nφτιάξεις τις δικές σου ιστορίες,\n\nπαιχνίδια,\n\nκαι κινούμενα σχέδια.\n\nΑς ξεκινήσουμε παίρνοντας μια εντολή \"κίνησης\".\n\nΜπορούμε να κάνουμε κλικ πάνω\nστην εντολή για να την εκτελέσουμε.\n\nΥπάρχουν πολλές άλλες εντολές για να διαλέξουμε.\nΔες τις διαφορετικές κατηγορίες.\n\nΑς πάμε στην κατηγορία Ήχος. και ας πάρουμε\nτην εντολή \"Παίξε ήχο μέχρι το τέλος\".\n\nΘα την ενώσω με την εντολή κίνησης\n\nΠότε θέλουμε αυτές οι εντολές να εκτελούνται;\n\nΠρόσθεσε μια εντολή συμβάντων\nστην αρχή του κώδικα.\n\nΘα προσθέσω την εντολή \"Όταν γίνει\nκλικ στην πράσινη σημαία\"\n\nΜπορείς εύκολα να αλλάξεις τη σειρά ή\nνα διαγράψεις τις εντολές όποτε θέλεις.\n\nΠρόσθεσε όσους χαρακτήρες θέλεις στο έργο σου,\nκαι προγραμμάτισε τον καθέναν τους.\n\nΔες τη σελίδα του Εκπαιδευτικού υλικού\nγια ιδέες και έμπνευση!"},{"language":"gla","text":"Le Scratch, cruthaichidh tu na sgeulachdan,\n\ngeamannan,\n\nagus beòthachaidhean agad fhèin.\n\nTòisicheamaid is sinn a’ togail bloca “gluais”.\n\n’S urrainn dhuinn briogadh air a’ bhloca gus a ruith.\n\nTha iomadh seòrsa bloca eile ann as urrainn dhut taghadh.\nThoir sùil air na roinnean-seòrsa.\n\nThèid sinn dhan roinn-seòrsa “Fuaim”\nagus togaidh sinn bloca “cluich fuaim gu lèir”.\n\nGreimichidh mi seo ris a’ bhloca “gluais” againn\n\nCuin a tha sinn airson na blocaichean còd seo a ruith?\n\nCuir bloca “Tachartas” ris a’ bhàrr airson an còd a chur gu dol.\n\nCuiridh mi bloca “Le briogadh air a’ bhratach uaine” ris.\n\n’S urrainn dhut òrdugh ùr a chur air na blocaichean seo\nno an sguabadh às uair sam bith.\n\nCuir na thogras tu a phearsachan ris a’ pròiseact agad\nagus cuir còd ris gach fear.\n\nThoir sùil air duilleag nan oideachaidhean airson beachdan is brosnachadh!"},{"language":"pol","text":"W Scratchu możesz zrobić swoje własne\n\ngry,\n\ni animacje.\n\nZacznijmy od chwycenia klocka \"przesuń\".\n\nMożemy kliknąć na klocek, aby go uruchomić.\n\nJest wiele innych klocków do wyboru.\nSprawdźmy pozostałe kategorie.\n\nPrzejdźmy do drugiej kategorii\ni chwyćmy klocek \"zagraj aż skończysz\".\n\nDołączmy tej klocek, do klocka ruchu.\n\nKiedy te klocki mają być uruchomione?\n\nDodajmy klocek z bloku \"zdarzenia\" na początek kodu.\n\nNa przykład klocek \"Kiedy kliknięto zieloną flagę\".\n\nW dowolnym momencie możemy łatwo przestawiać\nalbo usuwać klocki.\n\nMożemy także dodawać wielu bohaterów do naszego projektu \noraz tworzyć kod dla każdej z postaci.\n\nNa stronie z samouczkami znajdziesz różne propozycje i inspirujące projekty!"},{"language":"rum","text":"Cu Scratch îți poți crea propriile scenarii\n\njocuri,\n\nși animații\n\nSă începem prin a adăuga un bloc de tipul \"mergi\"\n\nPutem da click pe blocul de cod pentru a-l rula.\n\nSunt multe alte blocuri din care poți alege.\nÎncearcă diferitele categorii.\n\nSă mergem la categoria sunet\nși sa alegem blocul \"redă sunetul până la final\"\n\nO sa combin aceasta opțiune cu cea de \"mergi\".\n\nCând vrem ca aceste doua blocuri de cod să ruleze ?\n\nAdaugă un bloc de tip eveniment, deasupra celorlalte.\n\nVoi adăuga blocul \"când se dă click pe steagul verde\"\n\nPoți rearanja  blocurile de cod cu ușurința\nsau le poți șterge oricând dorești.\n\nAdaugă câte personaje dorești\nîn proiectul tău și animă-l pe fiecare cum dorești.\n\nAccesează pagina de tutoriale\npentru idei și inspirație!"},{"language":"vie","text":"Với Scratch, bạn có thể tạo ra câu chuyện của riêng bạn,\n\ntrò chơi,\n\nvà hiệu ứng chuyển động.\n\nHãy bắt đầu bằng cách thêm một khối \"di chuyển\".\n\nChúng ta có thể nhấn vào khối để thực thi.\n\nCó nhiều khối khác nhau cho bạn.\nHãy khám phá các thể loại khác nhau.\n\nHãy đi đến thể loại âm thanh,\nvà thêm khối \"Phát âm thanh đến khi kết thúc\".\n\nTôi sẽ gắn khối này vào khối di chuyển\n\nBạn muốn khi nào những khối này được thực thi?\n\nThêm một khối \"sự kiện\" lên đầu để bắt đầu thực thi đoạn lệnh.\n\nTôi sẽ thêm khối \"Khi nhấn vào cờ xanh\".\n\nBạn có thể dễ dàng sắp xếp lại,\nhoặc xoá chúng đi bất cứ khi nào.\n\nThêm nhiều nhân vật tùy ý\nvào dự án của bạn, và lập trình cho từng nhân vật.\n\nHãy truy cập trang Hướng dẫn để có thêm ý tưởng và cảm hứng!"},{"language":"rus","text":"С помощью Scratch ты можешь создавать собственные истории,\n\nигры,\n\nи анимации.\n\nНачнём с блока \"идти\". Перетащите его сюда.\n\nНажми на блок, чтобы запустить его\n\nТут столько классных блоков.\nПопробуй разные категории.\n\nДавай зайдём в категорию \"Звук\",\nи перетащим блок \"Играть звук до конца\"\n\nЯ прикреплю его к блоку \"Идти\".\n\nКогда мы хотим, чтобы этот код выполнялся?\n\nДобавь наверх блок из категории \"События\", чтобы запустить код.\n\nЯ добавлю блок \"Когда щёлкнут по зелёному флагу\".\n\nМожно изменить порядок и удалить блоки\n\nДобавь столько персонажей, сколько захочешь, и напиши код для каждого.\n\nЗайди во вкладку \"Уроки\" для новых идей."},{"language":"hun","text":"A Scratch segítségével saját történeteket,\n\njátékokat,\n\nés animációkat készíthetsz.\n\nKezdjük egy „menj” blokk megragadásával.\n\nRákattinthatsz a blokkra a futtatáshoz.\n\nSok más blokk közül választhatsz.\nVess egy pillantást a különféle kategóriákra.\n\nMenjünk a Hang kategóriába, és ragadjuk meg\na „játszd le és várd meg” blokkot.\n\nEzt hozzáillesztem a mozgás blokkunkhoz.\n\nMikor szeretnénk futtatni ezeket a blokkokat?\n\nAdjunk egy eseményblokkot a kód tetejére a program elindításához.\n\nÉn a „Zöld zászlóra kattintáskor” blokkot választottam.\n\nA blokkokat bármikor egyszerűen\nátrendezheted vagy törölheted.\n\nAkárhány karaktert felvehetsz a projektedbe,\nés mindegyiknek készíthetsz kódot.\n\nNézd meg a Gyakorlatok oldalt ötletekért és inspirációért!"},{"language":"nor","text":"Med Scratch kan du laga dine eigne forteljingar, –\n\n– spel –\n\n– og animasjonar.\n\nFørst legg me til ein «flytt»-kloss.\n\nMe kan trykkja på klossen for å køyra han.\n\nDu kan velja mellom mange ulike klossar.\nSjå gjennom alle klosskategoriane.\n\nGå til «Lyd»-kategorien og vel ein\n«spel lyden til han er ferdig»-kloss.\n\nEg festar han til «flytt»-klossen vår.\n\nNår vil me at klossane skal køyra?\n\nFest ein hendingskloss som første\nkloss for å starta programmet.\n\nEg brukar ein «når det grøne\nflagget vert trykt på»-kloss.\n\nDu kan enkelt endra rekkjefølgja\npå klossane eller ta dei vekk.\n\nLegg til så mange figurar du vil,\nog kod korleis dei skal oppføra seg.\n\nSjå på innføringssida for\nfleire idéar og inspirasjon!"},{"language":"per","text":"با اسکرچ می‌توانید داستان‎‌های خود را بسازید و\n\nو همینطور بازی ها\n\nو البته انیمیشن‌ها.\n\nبیایید از یک بلوک «حرکت» استفاده کنیم\n\nبا کلیک روی یک بلوک می توانیم آنرا اجرا کنیم.\n\nکلی بلاک دیگر هم وجود دارند.\nبقیه گروه‌های را هم امتحان کنید.\n\nبیایید به سراغ گروه «صداها» برویم.\nبلاک «پخش صدای __ وقتی که تمام شود» را بگیرید.\n\nمن این بلاک را به بلاک «حرکت» قبلیمان می‌چسبانم.\n\nحال باید بگوییم که چه موقعی باید این بلاک‌ها اجرا شوند؟\n\nبرای شروع این دستورات، یک بلوک «رویداد» را در بالا اضافه می‌کنیم.\n\nمن بلوک «وقتی پرچم سبز کلید شد» را اضافه می‌کنم.\n\nخیلی ساده می‌توانید این بلوک‌ها را جابه‌جا و یا حذف کنید.\n\nمی‌توانید هر چند تا شکلک که مایلید به پروژه‌تان اضافه کنید و برای هرکدام از آنها دستوراتی را بنویسید.\n\nصفحه «آموزش‌ها» را برای دیدن ایده‌های دیگر ببینید."},{"language":"por","text":"Com o Scratch, você pode fazer suas próprias histórias,\n\njogos\n\ne animações.\n\nVou começar arrastando um bloco \"mova\".\n\nPodemos clicar no bloco para testá-lo.\n\nExistem vários outros blocos.\nConfira as diferentes categorias.\n\nNa categoria \"Som\",\nvou selecionar: \"Toque o som até o fim\".\n\nVou encaixar esse bloco no bloco de movimento.\n\nTemos que indicar quando\neles devem ser ativados.\n\nEntão, um bloco de evento deve iniciar o código.\n\nVou usar o bloco: \"Quando bandeira verde for clicada\".\n\nVocê pode facilmente reordenar ou excluir os blocos a qualquer momento.\n\nAdicione quantos personagens quiser ao seu projeto, e programe cada um deles.\n\nConfira a página de Tutoriais para mais ideias e inspiração!"},{"language":"jpn","text":"Scratchで、自作の物語や、\n\nゲーム、\n\nおまけにアニメーションまで作れちゃう。\n\nじゃあ、”～歩動かす\"をつかむことから始めよう。\n\nブロックをクリックすると実行できるよ。\n\n他にも選択できるブロックが、数多くあるんだ。\n色々なカテゴリをチェックしてね。\n\n音のカテゴリーで、\n”終わるまで～の音を鳴らす”をつかんで、\n\n最初に加えた”～歩動かす”に\nくっつけよう。\n\nこの複数のコードを\nいつ実行したい?\n\nコードをスタートさせるには、\n一番上にイベントのブロックを加えます。\n\n”旗がクリックされたとき”を加えるよ。\n\nブロックの並び替えや削除は、\nいつでも簡単にできます。\n\n好きなようにキャラクター達を追加して、\nそれぞれにコードを加えてね。\n\nアイデア と ひらめき を求めて、\nチュートリアルをチェックしよう！"},{"language":"srp","text":"Са Скрачом можеш да правиш своје приче,\n\nигре,\n\nи анимације.\n\nПочећемо од блока \"покрет\".\n\nМожемо да кликнемо на блок да га активирамо.\n\nПостоји велики избор блокова. Истражи све категорије.\n\nПређимо на категорију звук, изабери и превуци на сцену блок \"Репродукуј звук до краја\"\n\nНаставићемо овај блок на претходни блок покрет.\n\nКада желимо да се ови блокови кода изврше?\n\nДодај блок догађај на врх да активираш код.\n\nЈа ђу додати блок \"Када је зелена застава кликнута\".\n\nУ било ком тренутку можеш да промениш распоред блокова или да их избришеш.\n\nМожеш да додаш колико год желиш ликова у пројекат и да програмираш сваки засебно.\n\nПотражи нове идеје и инспирације на страници са туторијалима."},{"language":"ger","text":"Mit Scratch kannst du deine eigenen Geschichten,\n\nSpiele,\n\nund Animationen erstellen.\n\nFangen wir mit einem \"gehe\"-Block an.\n\nWir können auf den Block klicken, um ihn auszuführen.\n\nEs gibt viele andere Blöcke.\nSchau dir die verschiedenen Kategorien an.\n\nGehen wir zu der Kategorie \"Klang\",\nund nehmen einen \"spiele Klang ganz\"-Block.\n\nIch tue ihn unter den anderen Block.\n\nWann soll dieser Code Block ausgeführt werden?\n\nFüge einen Ereignisblock an den Anfang hinzu.\n\nIch nehme den Block \"Wenn die grüne Flagge angeklickt\"\n\nDu kannst die Blöcke jederzeit\neinfach umsortieren oder löschen.\n\nFüge so viele Figuren wie du möchtest zu\ndem Projekt hinzu, und programmiere sie alle.\n\nSchau dir für Ideen und Inspiration die Tutorial-Seite an!"},{"language":"ara","text":"بواسطة سكراتش، يمكنك إنشاء قصصك الخاصة،\n\nألعابك،\n\nأو رسومك المتحركة.\n\nلنبدأ بسحب لبنة \"تحرك\".\n\nيمكننا النقر على اللبنة لتشغيلها.\n\nهناك لبنات كثيرة مختلفة لنختار منها.\nاطَّلع على مجموعات الأوامر المختلفة.\n\nلنذهب إلى مجموعة الأصوات\nونسحب لبنة \"شغل الصوت حتى النهاية\".\n\nسألصق هذه اللبنة مع لبنة تحرك\n\nمتى نريد تشغيل هذه اللبنات؟\n\nأضف لبنة من مجموعة الأحداث فوق هذه اللبنات لتبدأ بتشغيل البرنامج.\n\nسأضف لبنة \"عند نقر العلم الأخضر\".\n\nيمكنك ببساطة إعادة ترتيب اللبنات\nأو مسح هذه اللبنات في أي وقت.\n\nأضف إلى مشروعك أي شخصيات ترغب بها،\nوبرمج كل واحدة منها.\n\nاطَّلع على صفحة الدروس التعليمية لمزيد من الأفكار والإلهامات!"},{"language":"kur","text":"Tu dikarî bi Scratchê çîrokên xwe çê bike,\n\nlîstikan\n\nû anîmasyonan.\n\nKa were em bi bloka \"bilivîne\"yê dest pê bikin.\n\nEm dikarin bi tikandina blokê wê bixebitînin.\n\nJi bo hilbijartinê gelek blok hene. Kategoriyên ji hev cuda, kontrol bikî.\n\nKa were em herin kategoriya deng û bloka \"heta deng biqede lêxe\"yê bistînin.\n\nEz ê vêya li bloka livandinê bizeliqînim.\n\nTu dixwazî ev blokên kodan kengî bixebitin?\n\nJi bo kod dest pê bike, blokeke bûyerê tevlî bike.\n\nEz ê bloka \"Dema ala kesk hat tikandin\"ê\ntevlî bikim.\n\nTu dikarî cihên van blokan biguherînî û li gor daxwaza dile xwe bi hêsanî jê bibî.\n\nHeta tu dixwazî karekteran tevlî bikî û wan yek bi yek bikodîne.\n\nJi bo zêdetir fikir û îlhamê çav li rûpela Fêrgehê bigerîne!"},{"language":"lit","text":"Su Scratch gali kurti istorijas, komiksus,\n\nžaidimus,\n\nir animacijas.\n\nPradėkime nuo \"eik\" bloko.\n\nSpustelėjus ant bloko, jis bus paleidžiamas.\n\nGali rinktis iš daugelio skirtingų blokų-komandų.\nPatikrink visas kategorijas!\n\nPažiūrėkime į garso kategoriją, ir išbandykime\n\"Grok garsą kol pasibaigs\" bloką.\n\nPrijungsiu jį prie \"eik\" bloko.\n\nKada gi norime, kad šios komandos būtų vykdomos?\n\nPrijunkime įvykio bloką viršuje komandų aktyvavimui.\n\nAš pridedu \"Kai spustelta žalia vėliavėlė\" bloką.\n\nBet kada gali lengvai apkeisti\nar ištrinti blokus.\n\nPridėk, kiek nori veikėjų į savo projektą,\nir suprogramuok kiekvieną iš jų\n\nIšbandyk Pamokų puslapį - ten rasi įvairių idėjų ir įkvėpimo!"},{"language":"geo","text":"Scratch-ის მეშვეობით შენ შეგიძლია შექმნა საკუთარი ამბები,\n\nთამაშები,\n\nდა მულტფილმები.\n\nმოდი, გადაადგილების ბლოკით დავიწყოთ.\n\nდავაჭიროთ ბლოკს მის ასამუშავებლად.\n\nმრავალრიცხოვანი ბლოკებიდენ სხვების არჩევაც შეგიძლია.\nსინჯე სხვა კატეგორიები.\n\nგადავინაცვლოთ ხმის კატეგორიაში\nდა ავირჩიოთ ბლოკი \"დაუკარი ხმა დასრულებამდე\".\n\nმე მას გადაადგილების ბლოკს შევუერთებ\n\nროდის უნდა ამოქმედდენ ეს ბლოკები?\n\nზევიდან მოვლენის ბლოკი დავამატოთ პროგრამის ჩასართავად.\n\nმე დავამატებ ბლოკს \"როდესაც მწვანე დროშაა დაჭერილი\".\n\nშენ ნებისმიერ დროს ადვილად შეგიძლია\nადგილი შუცვალო ამ ბლოკებს ან წაშალო ისინი.\n\nსურვილისამებრ, დაუმატე პროექტს სხვა პერსონაჟები\nდა შეადგინე პროგრამა თითოეული მათგანისათვის.\n\nიხილე გაკვეთილების გვერდი ახალი იდეებისა და შთაგონებისათვის!"},{"language":"mao","text":"Scratchwanmi, rurawaqmi willakuykunata,\n\npukllaykunata,\n\nhinaspa kawsarichiytapas.\n\nqallarisun hapirispa \"kuyuriy\" champawan.\n\nrurachwanmi klikyaykuspa champapi qallarinampaq.\n\nkanmi achka champakuna akllanapaq. qawariy llapan rikchaq katiquriyamanta.\n\nhakuchik luqyay katiquriyaman, hinaspa apiriy huk champata \"luqyay tukunaykikama\" nisqanta.\n\nkayta churasunchik kuyuy champayninchikpi\n\nhayka punitaq kallparinqa kay kudiqu champanchikqa?\n\nyapay huk champata iwintuykipi hanay kudiquyki hinaspa qallari.\n\nyapay champata \"klikyaykupti qumir wifalapi\".\n\nqam facillata allichawaq utaq pichawaqpas kay champakunata ima punchawpipas.\n\nyapay munasqayki runaniraqta llamkay, hinaspa llamkay llapanta.\n\nqaway tuturialkunta yanapasunaykipaq munasqaykipi!"},{"language":"zul","text":"KuScratch uyakwazi ukuzenzela\nezakho izindaba\n\nimidlalo,\n\nnama opopayi.\n\nSizoqala ngokuthatha\nibholukisi \"lokunyakaza\".\n\nSingacipiza ibholukisi ukubona\nukuthi lisebenza kanjani.\n\nManingi amabholukisi ongakhetha kuwona.\nBheka ezinye zezinhlelo zamabhulokisi.\n\nAsichofozeni uhlelo lwezemisindo sithathe ibhulokisi \"Dlala umsindo uzu uqede\".\n\nNgizothatha lokhu ngikuyise kwibhulokisi\nlokunyakaza lethu\n\nSifuna ukuthi lamakhodi\namabhulokisi asebenzenini?\n\nNezelela ngokufaka ibhulokisi lezehlakalo phezulu kuze uqale ikhodi.\n\nNgizonezelela ngebhulokisi\n\"uma ifulegi elikuhlaza lichofoziwe.\n\nKulula ukuhlela kabusha\nnoma ukususa lamabhulokisi noma isiphi isikhathi.\n\nNezezela ngokufaka abanye abalingiswa obafunayo kwi project yakho\nbezele ikhodi ngamunye.\n\nBheka ikhasi lama Tutorials uthole\namasu no gqozu!"},{"language":"tur","text":"Scratch ile kendi hikayelerinizi yapabilirsiniz.\n\noyunlar,\n\nve animasyonlar.\n\nHadi \"move\" bloğuyla başlayalım.\n\nBloğa tıklayarak onu çalıştırabiliriz.\n\nSeçebileceğiniz birçok blok var.\nDeğişik kategorileri kontrol edin.\n\nHaydi ses kategorisine gidelim, ve\n\"sesini bitene kadar çal\" bloğunu alalım.\n\nBunu hareket blokumuza yapıştıracağım.\n\nBu kod bloklarının ne zaman çalışmasını istiyoruz?\n\nKodu başlatmak için\nüstüne bir olay bloğu ekleyin.\n\n\"Yeşil bayrak tıklandığında\" bloğunu ekleyeceğim.\n\nBu blokların yerlerini değiştirip\nistediğiniz zaman kolayca silebilirsiniz.\n\nİstediğiniz kadar çok karakter ekleyip\nher birini kodlayın.\n\nFikirler ve ilham almak için\nÖğreticiler sayfasına göz atın!"}],"hls_enabled":true,"embed_options":{"volumeControl":"true","fullscreenButton":"true","controlsVisibleOnLoad":"false","playerColor":"7f7f7f","bpbTime":"false","plugin":{"captions-v1":{"language":"","onByDefault":"true","on":"true","async":"true"}},"anonymizeIp":true,"vulcan":"true","videoQuality":"","branding":"false","showCustomerLogo":"false","unalteredStillImageAsset":{"url":"https://embed-ssl.wistia.com/deliveries/9e886487951fdf158dc0823801eb0aa7b90a8c57.jpg","width":"1280","height":"720"},"spherical":"false","qualityControl":"true","playbackRateControl":"true","autoPlay":"false","silentAutoPlay":"allow","endVideoBehavior":"reset","settingsControl":"true","playButton":"true","smallPlayButton":"true","playbar":"true","playsinline":true}},"options":{}};
